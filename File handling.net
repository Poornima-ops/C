using System;
using System.IO;

class FileHandling
{
    static void Main()
    {
        string path = "testfile.txt";
        
        // Write to file
        File.WriteAllText(path, "Hello, this is a test file.");

        // Read from file
        string content = File.ReadAllText(path);
        Console.WriteLine("File Content: " + content);
    }
}
